//using set method it's time complexitiy is O(m * log(m) + n * log(n))
int doUnion(int a[], int n, int b[], int m) 
    {
        set<int> s;//defining set 
    for(int i=0;i<n;i++)
    {
        s.insert(a[i]);
    }
    for(int i=0;i<m;i++)
    {
        s.insert(b[i]);
    }
   return s.size(); 
        //code here
    }
    
